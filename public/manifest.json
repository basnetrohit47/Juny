{
    "manifest_version": 3,
    "name": "Juny",
    "version": "1.0.3",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
      
    "description": "Make your tab beautiful and productive with favourite sites and todo list items ",
   
    "permissions": ["storage",
    "tabs"],
   
    "host_permissions": [
    "<all_urls>"
  ],
    "chrome_url_overrides": {
      "newtab": "index.html"
    }
  }